<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".activitys.commissions.WithdrawalSuccessActivity"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <include
        android:id="@+id/include_title"
        layout="@layout/include_common_title"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>
    <ImageView
        android:id="@+id/iv_success"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/ic_withdrawal_success"
        app:layout_constraintTop_toBottomOf="@id/include_title"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="48dp"/>
    <TextView
        android:id="@+id/tv_apply_success"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/str_apply_success"
        app:layout_constraintTop_toBottomOf="@id/iv_success"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="@dimen/d_order_item_pad_t"
        style="@style/tv_logistics_status_black"/>
    <TextView
        android:id="@+id/tv_wait_review"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/str_wait_review"
        app:layout_constraintTop_toBottomOf="@id/tv_apply_success"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="@dimen/d_order_my_pad_b"
        android:textSize="@dimen/d_font_prompt"
        android:textColor="@color/c_order_sku"/>
    <View
        android:id="@+id/v_dividing_1"
        style="@style/v_dividing_common_small_w_0"
        app:layout_constraintTop_toBottomOf="@id/tv_wait_review"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="@dimen/d_order_my_pad_b"
        android:layout_marginRight="@dimen/d_order_my_pad_b"
        android:layout_marginTop="@dimen/d_order_detail_pay_h"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/v_dividing_1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="@dimen/d_order_my_pad_b">
        <include layout="@layout/include_withdrawal_detail_infos"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <TextView
        android:id="@+id/btn_reapply"
        android:layout_width="match_parent"
        android:layout_height="@dimen/d_order_detail_bottom_btn_h"
        style="@style/btn_dialog_refund_reason_emphasize"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="@dimen/d_order_my_pad_b"
        android:layout_marginRight="@dimen/d_order_my_pad_b"
        android:layout_marginBottom="@dimen/d_after_sale_bottom_btn_mar_t"
        android:text="@string/str_finish" />
</androidx.constraintlayout.widget.ConstraintLayout>