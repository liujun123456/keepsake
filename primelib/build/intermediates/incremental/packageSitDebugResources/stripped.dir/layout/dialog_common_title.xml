<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingLeft="@dimen/d_order_my_pad_b"
    android:paddingRight="@dimen/d_order_my_pad_b"
    android:paddingTop="@dimen/d_dialog_pad_t"
    android:paddingBottom="@dimen/d_dialog_pad_t">
    <TextView
        android:id="@+id/tv_dialog_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/str_select_time"
        style="@style/tv_after_sale_dialog_prompt"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>
    <ImageView
        android:id="@+id/iv_dialog_close"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/ic_dialog_close"
        app:layout_constraintTop_toTopOf="@id/tv_dialog_title"
        app:layout_constraintRight_toRightOf="parent"
        android:scaleType="centerInside"/>
</androidx.constraintlayout.widget.ConstraintLayout>